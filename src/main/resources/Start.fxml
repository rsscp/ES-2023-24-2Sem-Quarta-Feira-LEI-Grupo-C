<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<VBox fx:id="root" spacing="10" alignment="CENTER" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="org.project.ui.StartController" prefHeight="400.0" prefWidth="600.0">
    <StackPane minHeight="100" minWidth="250">
        <VBox spacing="10" alignment="CENTER" maxWidth="150">
            <Text fx:id="fileselecterror" style="-fx-fill:red; -fx-font-size:12" visible="false" managed="false">Failed to open selected file</Text>
            <Button onAction="#chooseLocalLectureFile" textAlignment="CENTER" wrapText="true" maxWidth="10000" minHeight="50">Ficheiro Lecture local</Button>
            <Button onAction="#chooseLocalRoomFile" textAlignment="CENTER" wrapText="true" maxWidth="10000" minHeight="50">Ficheiro Room local</Button>
            <Button onAction="#openUrlInput" textAlignment="CENTER" wrapText="true" maxWidth="10000" minHeight="50">Ficheiro em reposit√≥rio remoto</Button>
        </VBox>
    </StackPane>
</VBox>
